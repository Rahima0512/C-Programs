void selectionSort(int arr[], int n)
{
    int i;
    
    for(i=0;i<n-1;i++)
    {
        
        for(int j=i+1;j<n;j++)
        {
            int temp=arr[i];
            if(arr[i]>=arr[j])
            {
                arr[i]=arr[j];
                arr[j]=temp;
                
            }
        }
    }
}
