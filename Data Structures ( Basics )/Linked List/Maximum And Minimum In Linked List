int maximum(Node *head)
{
    int max=head->data;
    Node *curr=head;
    while(curr!=nullptr)
    {
        if(curr->data>max)
        {
            max=curr->data;
        }
        curr=curr->next;
        
    }
    return max;
    
}

int minimum(Node *head)
{
    int min=head->data;
    Node *curr=head;
    while(curr!=nullptr)
    {
        if(curr->data<min)
        {
            min=curr->data;
        }
        curr=curr->next;
        
    }
    return min;
}
