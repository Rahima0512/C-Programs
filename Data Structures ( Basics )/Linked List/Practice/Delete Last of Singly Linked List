#include <bits/stdc++.h> 
using namespace std; 

struct Node{
    int data;
    Node* next;
    Node(int x){
        data=x;
        next=NULL;
    }
};

void printlist(Node *head){
    Node *curr=head;
    while(curr!=NULL){
        cout<<curr->data<<" ";
        curr=curr->next;
    }cout<<endl;
}

Node *del_last_element(Node *head){
    if(head==nullptr)
    {
        return nullptr;
    }
    if(head->next==nullptr)
    {
        delete head;
        return nullptr
    }
    else
    {
        Node *temp=head;
        while(temp->next->next!=nullptr)
        {
            temp=temp->next;
            
        }
        delete (temp->next);
        temp->next=nullptr;
        return head;
    }
    
}
int main() 
{ 
	Node *head=new Node(10);
	head->next=new Node(20);
	head->next->next=new Node(30);
	printlist(head);
	head=del_last_element(head);
	printlist(head);
	
	return 0;
} 
