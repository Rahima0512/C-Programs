
#include <bits/stdc++.h>
using namespace std;

struct node
{
    int data;
    node * next,*prev;
    node (int x)
    {
        data=x;
        next=NULL;
        prev=nullptr;
    }
        
};

int main()
{
    node *head,*temp1,*temp2;
    head=new node(10);
    temp1=new node(20);
    temp2=new node(30);
    head->next=temp1;
    temp1->next=temp2;
    temp1->prev=head;
    temp2->prev= temp1;
    cout<<"List created\n";
    
    
    
}

