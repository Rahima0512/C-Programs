int main() {
    int sum=0,numbers[3]={1,2,3};
    for(int x:numbers)
    {
        sum=sum+x;
        
    }
    std::cout<<sum;

    return 0;
}







        or

int arraySum(int numbers[], int size){
    int sum=0;
    for(int i=0;i<size;i++)
    {
        sum=sum+numbers[i];
    }
    return sum;
    
    
    
}
