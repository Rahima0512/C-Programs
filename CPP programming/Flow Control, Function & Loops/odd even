    cout<<"\n";
    
    for(int i=1;i<=a;i++)
    {
        cout<<i<<"\t";
        if(i%2==0)
        {
            even
            cout<<"even"<<"\n";
        }
        else
        {
            cout<<"odd"<<"\n";
        }
        
