#include<bits/stdc++.h>
#include<cmath>
using namespace std;

void allfactors(int num)
{
    int i;
    for(i=1;i*i<=num;i++)
    {
        if(num%i==0)
        {
            cout<<i<<"\t";
            
            
        }
    }
    for( ;i>0;i--)
    {
        if(num%i==0)
        {
            cout<<num/i<<"\t";
            
            
        }
    }
    
}



int main()
{
    int num;
    cout<<"Enter a number\t";
    cin>>num;
    cout<<endl;
    allfactors(num);
    return 1;
    
}
